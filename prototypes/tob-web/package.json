{
  "name": "tob-web",
  "version": "2.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/bcgov/TheOrgBook.git"
  },
  "description": "TheOrgBook web interface component",
  "license": "Apache-2.0",
  "scripts": {
    "ng": "ng",
    "start": "node preinstall.js && ng serve -o --base-href ${NG_BASE_HREF:-/} --deploy-url ${NG_DEPLOY_URL:-/} --configuration ${NG_ENV:-development}",
    "build": "ng build --prod --base-href ${NG_BASE_HREF:-/} --deploy-url ${NG_DEPLOY_URL:-/} ${NG_BUILD_FLAGS:-}",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "preinstall": "node preinstall.js",
    "postinstall": "ng build --prod --base-href ${NG_BASE_HREF:-/} --deploy-url ${NG_DEPLOY_URL:-/} ${NG_BUILD_FLAGS:-}"
  },
  "dependencies": {
    "@angular/animations": "~5.2.0",
    "@angular/cli": "~6.0.0",
    "@angular/common": "~5.2.0",
    "@angular/compiler": "~5.2.0",
    "@angular/core": "~5.2.0",
    "@angular/forms": "~5.2.0",
    "@angular/http": "~5.2.0",
    "@angular/platform-browser": "~5.2.0",
    "@angular/platform-browser-dynamic": "~5.2.0",
    "@angular/router": "~5.2.0",
    "@ng-bootstrap/ng-bootstrap": "~1.0.0",
    "@ngx-translate/core": "~8.0.0",
    "@ngx-translate/http-loader": "^2.0.0",
    "bootstrap-theme": "cywolf/bootstrap-theme",
    "compare-versions": "~3.1.0",
